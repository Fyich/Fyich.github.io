#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/C:/Kshy07/Blogs/node_modules/.store/tldts@6.1.86/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/C:/Kshy07/Blogs/node_modules/.store/tldts@6.1.86/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tldts@6.1.86/node_modules/tldts/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../../../tldts@6.1.86/node_modules/tldts/bin/cli.js" "$@"
fi
